# Production Environment

# Supabase (Production)
VITE_SUPABASE_URL=https://your-production-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-production-anon-key

# Redis (Production)
VITE_REDIS_URL=redis://production-redis:6379

# Bitrix24 (Production Webhook)
VITE_BITRIX_WEBHOOK=https://promobrindes.bitrix24.com.br/rest/1/production-webhook-key

# Analytics (Production)
VITE_GA_MEASUREMENT_ID=G-PRODUCTION-ID
VITE_MIXPANEL_TOKEN=production-mixpanel-token

# Email (Production)
VITE_SENDGRID_API_KEY=SG.production-sendgrid-key

# Error Tracking (Production)
VITE_SENTRY_DSN=https://production-sentry-dsn@sentry.io/project-id
VITE_SENTRY_ENVIRONMENT=production
VITE_SENTRY_TRACES_SAMPLE_RATE=0.1

# Payment (Live Keys)
VITE_STRIPE_PUBLIC_KEY=pk_live_production-stripe-key
VITE_MERCADOPAGO_PUBLIC_KEY=production-mercadopago-key

# Feature Flags
VITE_ENABLE_AI_RECOMMENDATIONS=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_A11Y_FEATURES=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_MODE=false

# Application
VITE_APP_NAME=Gifts Store
VITE_APP_URL=https://gifts-store.com.br
VITE_API_TIMEOUT=30000

# Encryption (Production Key - SECURE!)
VITE_ENCRYPTION_KEY=production-32-char-secure-key

# WhatsApp (Production)
VITE_WHATSAPP_BUSINESS_ID=production-business-id
VITE_WHATSAPP_ACCESS_TOKEN=production-access-token

# Performance
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_CACHING=true
VITE_CDN_URL=https://cdn.gifts-store.com.br

# Security
VITE_ENABLE_CSRF_PROTECTION=true
VITE_ENABLE_RATE_LIMITING=true
VITE_MAX_REQUESTS_PER_MINUTE=100
